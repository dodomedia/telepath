{
    "name": "tii/telepath",
    "description": "PHP Library for creating Telegram Bots",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tii\\Telepath\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tii\\TelepathTest\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Tii",
            "email": "mail@tii.one"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "cache/simple-cache-bridge": "^1.2"
    },
    "bin": [
        "./telepathy"
    ],
    "require-dev": {
        "spatie/ray": "^1.34",
        "symfony/cache": "^6.0"
    }
}
